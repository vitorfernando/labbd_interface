<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>GetGenres</servlet-name>
        <servlet-class>Controller.GetGenres</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetLanguages</servlet-name>
        <servlet-class>Controller.GetLanguages</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddGenres</servlet-name>
        <servlet-class>Controller.AddGenres</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddLanguages</servlet-name>
        <servlet-class>Controller.AddLanguages</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Consulta1</servlet-name>
        <servlet-class>Controller.Consulta1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Consulta2</servlet-name>
        <servlet-class>Controller.Consulta2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetActors</servlet-name>
        <servlet-class>Controller.GetActors</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetDirectors</servlet-name>
        <servlet-class>Controller.GetDirectors</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Teste</servlet-name>
        <servlet-class>Controller.Teste</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetGenres</servlet-name>
        <url-pattern>/GetGenres</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetLanguages</servlet-name>
        <url-pattern>/GetLanguages</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddGenres</servlet-name>
        <url-pattern>/AddGenres</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddLanguages</servlet-name>
        <url-pattern>/AddLanguages</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Consulta1</servlet-name>
        <url-pattern>/Consulta1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Consulta2</servlet-name>
        <url-pattern>/Consulta2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetActors</servlet-name>
        <url-pattern>/GetActors</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetDirectors</servlet-name>
        <url-pattern>/GetDirectors</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Teste</servlet-name>
        <url-pattern>/Teste</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            90
        </session-timeout>
    </session-config>
</web-app>
